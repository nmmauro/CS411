
doctype html
html
  head
    style.
      #map {
      height: 400px;
      width: 100%;
      }
  body
    h3 My Google Maps Demo
    #map
    script.
      function initMap() {
      var uluru = {lat: -25.363, lng: 131.044};
      var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 4,
      center: uluru
      });
      var marker = new google.maps.Marker({
      position: uluru,
      map: map
      });
      }
    script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyB8fW8f_Bs1QvWKZajlOW6fItDTP6ol_XM
content_copy&callback=initMap')
